# Example environment variables for VictoriaLogs HTTP transport livetest

# Required: The VictoriaLogs instance URL (e.g., http://localhost:9428)
VICTORIA_LOGS_URL=http://localhost:9428

# Optional: Enable gzip compression (true/false)
VICTORIA_LOGS_COMPRESSION=true

# Optional: Max number of retries
VICTORIA_LOGS_MAX_RETRIES=3

# Optional: Retry delay in ms
VICTORIA_LOGS_RETRY_DELAY=1000

# Optional: Respect rate limiting (true/false)
VICTORIA_LOGS_RESPECT_RATE_LIMIT=true

# Optional: Enable batch sending (true/false)
VICTORIA_LOGS_ENABLE_BATCH=true

# Optional: Batch size
VICTORIA_LOGS_BATCH_SIZE=100

# Optional: Batch send timeout in ms
VICTORIA_LOGS_BATCH_TIMEOUT=5000
