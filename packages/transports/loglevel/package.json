{
  "name": "@loglayer/transport-loglevel",
  "description": "loglevel transport for the LogLayer logging library.",
  "version": "1.0.7",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": "loglayer/loglayer.git",
  "author": "Theo Gravity <theo@suteki.nu>",
  "keywords": [
    "logging",
    "log",
    "loglayer",
    "loglevel",
    "transport"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "test": "vitest --run",
    "build:dev": "hash-runner",
    "clean": "rm -rf .turbo node_modules dist",
    "lint": "biome check --write --unsafe src && biome format src --write && biome lint src --fix",
    "verify-types": "tsc --noEmit",
    "livetest": "tsx src/__tests__/livetest.ts"
  },
  "dependencies": {
    "@loglayer/transport": "workspace:*"
  },
  "devDependencies": {
    "loglayer": "workspace:*",
    "hash-runner": "2.0.1",
    "@types/node": "22.10.10",
    "@internal/tsconfig": "workspace:*",
    "tsup": "8.3.5",
    "typescript": "5.7.3",
    "vitest": "3.0.4",
    "tsx": "4.19.2"
  },
  "peerDependencies": {
    "loglevel": "*"
  },
  "bugs": "https://github.com/loglayer/loglayer/issues",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://loglayer.dev",
  "packageManager": "pnpm@9.12.2"
}
